<div class="feed-list">
  <ul>
    <li>
      <a href="{{ siteConfig.feed.excerpts.path }}">
        <svg class="bi"><use href="#icon-rss" /></svg>
        <span class="name">{{ siteConfig.feed.excerpts.title }}</span>
      </a>
    </li>
    <li>
      <a href="{{ siteConfig.feed.full.path }}">
        <svg class="bi"><use href="#icon-rss" /></svg>
        <span class="name">{{ siteConfig.feed.full.title }}</span>
      </a>
    </li>
    <li>
      <a href="{{ siteConfig.json.excerpts.path }}">
        <svg class="bi"><use href="#icon-json-feed" /></svg>
        <span class="name">{{ siteConfig.json.excerpts.title }}</span>
      </a>
    </li>
    <li>
      <a href="{{ siteConfig.json.full.path }}">
        <svg class="bi"><use href="#icon-json-feed" /></svg>
        <span class="name">{{ siteConfig.json.full.title }}</span>
      </a>
    </li>
    <li>
      <a href="{{ siteConfig.twtxt.path }}">
        <span class="twtxt-logo">
          <svg class="bi"><use href="#icon-twtxt" /></svg>
        </span>
        <span class="name">{{ siteConfig.twtxt.title }}</span>
      </a>
    </li>
  </ul>
</div>
