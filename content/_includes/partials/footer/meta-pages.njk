{% if siteConfig.metaPages %}
  <ul class="nav col-md-4 justify-content-end list-unstyled d-flex">
    {% for item in siteConfig.metaPages %}
      <li class="ms-3">
        <a rel="me" class="text-body-secondary" href="{{ item.url }}" title="{{ item.title }}"><span>{% icon item.icon, 'class'='bi' %}</span></a>
        </li>
    {% endfor %}
  </ul>
{% endif %}
